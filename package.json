{
  "name": "translate-numbers",
  "version": "1.0.5",
  "description": "Transalte any number into english or vice-versa.",
  "author": "Mohit Agrawal",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsup",
    "ci": "npm run build && npm run check-format && npm run lint && npm run test",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "lint": "tsc",
    "test": "vitest run",
    "dev": "vitest"
  },
  "keywords": [
    "transalte",
    "numbers"
  ],
  "homepage": "https://github.com/mohitagrawal939/translate-numbers",
  "bugs": {
    "url": "https://github.com/mohitagrawal939/translate-numbers/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohitagrawal939/translate-numbers.git"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "prettier": "^3.4.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  }
}
